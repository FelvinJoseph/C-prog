#include<stdio.h>
#include<string.h>
void main()
{
    char pass[20]="Pass@213",word[20];
    do{
        printf("enter the password:");
        scanf("%s",word);
        if(strcmp(pass,word)==0)
        {
            printf("correct password\n");
            break;
        }
        else
        printf("try again\n");
    }while(1);
}
