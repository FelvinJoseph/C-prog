#include<stdio.h>
#include<ctype.h>
int main()
{
    char ch;
    printf("Enter the character: ");
    scanf("%c",&ch);
    ch=tolower(ch);
    switch(ch)
    {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':printf("It is a vowel...");
                break;
        default:printf("It is not a vowel...");
                break;
    }
    return 0;
}
